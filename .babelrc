{
   "plugins": [
      [
         "module:react-native-dotenv",
         {
            "moduleName": "@env",
            "path": ".env",
            "blocklist": null,
            "allowlist": ["API_URL", "FACULTY_ID"],
            "blacklist": null, // DEPRECATED
            "whitelist": null, // DEPRECATED
            "safe": false,
            "allowUndefined": true,
            "verbose": false
         }
      ]
   ]
}
